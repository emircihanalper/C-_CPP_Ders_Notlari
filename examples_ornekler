if (saat>0 && saat<=24)
{
    if (saat>0 && saat<=8)
    {
        cout<<"Uyku vakti";
    }
    else if (saat>8 && saat<=12)
    {
        cout<<"Günaydın";
    }
    else if (saat>12 && saat<=18)
    {
        cout<<"İyi Günler";
    }
    else if (saat>18 && saat<=24)
    {
        cout<<"İyi Geceler";
    }
}
    else
    {
        cout<<"0-24 arası değer giriniz.";
    }

