  cin>>sayi1;
  cin>>sayi2;
  if (sayi1>sayi2)
  {
    cout<<sayi1;
  }
  else if (sayi1<sayi2)
  {
    cout<<sayi2;
  }
  else
  {
    cout<<"Sayılar eşit";
  }
  cout<<"\n\n";
/////////////////////////////////////////////////////////////////////////////////////////////////////////
//0-100 arasındaki notları 5 lik sisteme çevirme
//girilen not o-25 arası 0 / 26-44 arası 1 / 45-54 arası 2 / 55-69 arası 3 / 70-84 arası 4 / 85-100 arası 5
int notunuz;
cout<<"Notunuzu giriniz: ";
cin>>notunuz;
if (notunuz>0 && notunuz<=100)
{
    if (notunuz>0 && notunuz<=25)
    {
        cout<<"Notunuz: 0";
    }
    else if (notunuz>25 && notunuz<=44)
    {
        cout<<"Notunuz: 1";
    }
    else if (notunuz>44 && notunuz<=54)
    {
        cout<<"Notunuz: 2";
    }
    else if (notunuz>54 && notunuz<=69)
    {
        cout<<"Notunuz: 3";
    }
    else if (notunuz>69 && notunuz<=84)
    {
        cout<<"Notunuz: 4";
    }
    else if (notunuz>84 && notunuz<=100)
    {
        cout<<"Notunuz: 5";
    }
}
    else
    {
        cout<<"Lütfen 0-100 arası değer giriniz.";
    }

