  cin>>say1;
  cout<<(say1%2);  // () içindeki ifade şart sağlandığı zaman 1, değilse 0  yazar
  cout<<"\n\n";

/////////////////////////////////////////////////////////////////////////////////////////////////////////
  //klavyeden girilen 2 sayıdan büyük olanını ekrana yazan c++ komutları
  int sayi1,sayi2;
