//////////////////////////////////////////////////////////////////////
  string ad="Emir Cihan";
  string soyad="Alper";
  int yas=36;
  cout<<ad<<endl;
  cout<<soyad;
  cout<<"\n";
  cout<<yas;
  cout<<endl<<"Adınız: "<<ad<<endl<<"Soyadınız: "<<soyad<<"\n"<<"Yaşınız: "<<yas;
  cout<<"\n\n";

 ///////////////////////////////////////////////////////////////////////////////////////////////////////// 
  int a=10,b=20,c=30;
  cout<<"a+b+c= "<<a+b+c;
  cout<<"\n\n";

/////////////////////////////////////////////////////////////////////////////////////////////////////////  
  //dairenin alanı pi*r*r
  //pi 3,14 olacak r de 5 olacak
  const double pi =3.14;     // double yerine float ta olur. "const" sabit değer vermek için.
  int r=5;
  cout<<"Dairenin alanı: "<<pi*r*r;
  cout<<endl<<"Çemberin çevresi: "<<2*pi*2;
  //kare kenar uzubluğu a=10
  //dikdörtgen kenar uzunlukları a=5, b=10
  cout<<endl<<"Karenin alanı: "<<a*a;
  cout<<endl<<"Karenin çevresi: "<<4*a;
  cout<<endl<<"Dikdörtgenin alanı: "<<a*b;
  cout<<endl<<"Dikdötgenin çevresi: "<<a+a+b+b;
  cout<<"\n\n";

/////////////////////////////////////////////////////////////////////////////////////////////////////////  
  string adiniz,soyadiniz;
  int yasiniz;
  cout<<"Lütfen adınızı giriniz: ";
  cin>>adiniz;		//cin veri girişi için kullanılır.
  cout<<endl<<"Lütfen soyadınızı giriniz: ";
  cin>>soyadiniz;
  cout<<endl<<"Lütfen yaşınızı giriniz: ";
  cin>>yasiniz;
  cout<<endl<<"isminiz: "<<adiniz<<endl<<"Soyisminiz: "<<soyadiniz<<endl<<"Yaşınız: "<<yasiniz;
  cout<<"\n\n";

/////////////////////////////////////////////////////////////////////////////////////////////////////////  
  //const float pi=3.14; ==>pi zaten tanımlı
  int yr,x,y,z;
  cout<<"Lütfen dairenin yarıçapını (yr) giriniz: ";
  cin>>yr;
  cout<<endl<<"Lütfen karenin kenar uzunuluğunu (x) giriniz: ";
  cin>>x;
  cout<<endl<<"Lütfen karenin kenar uzunuluklarını (y ve z) giriniz: ";
  cin>>y;
  cin>>z;
  cout<<endl<<"Dairenin alanı: "<<pi*yr*yr<<endl<<"Çemberin çevresi: "<<2*pi*yr<<endl<<"Karenin alanı: "<<x*x<<endl<<"Karenin çevresi: "<<4*x<<endl<<"Dikdörtgenin alanı: "<<y*z<<endl<<"Dikdörtgenin çevresi: "<<y+y+z+z;
  cout<<"\n\n";

/////////////////////////////////////////////////////////////////////////////////////////////////////////  
  int aa=5,bb=10;
  cout<<aa+bb<<endl;	//toplama
  cout<<aa-bb<<endl;	//çıkarma
  cout<<aa*bb<<endl;	//çarpma
  cout<<aa/bb<<endl;	//bölme
  cout<<aa++<<endl;	//önce yaz sonra topla yani 5 yaz ve 1 artır
  cout<<++aa<<endl;	//önce 1 artır sonra yaz yani 7
  cout<<"\n\n";

/////////////////////////////////////////////////////////////////////////////////////////////////////////
