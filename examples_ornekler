////////////////////////////////////////////////////////////////////////////////////////////////////////
int gun;
cout<<"Lütfen gün numarası giriniz (1-7)";
cin>>gun;
switch (gun)
{
    case 1:
    cout<<"Pazartesi";
    break;
    case 2:
    cout<<"Salı";
    break;
    case 3:
    cout<<"Çarşamba";
    break;
    case 4:
    cout<<"Perşembe";
    break;
    case 5:
    cout<<"Cuma";
    break;
    case 6:
    cout<<"Cumartesi";
    break;
    case 7:
    cout<<"Pazar";
    break;
    default:
    cout<<"Lütfen 1-7 arası gün giriniz.";
    break;
}

////////////////////////////////////////////////////////////////////////////////////////////////////////
